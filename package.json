    "babel-register": "^6.24.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "html-webpack-plugin": "^2.26.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-map-gl": "^3.2.0",
    "storybook-addon-material-ui": "^0.7.4",
